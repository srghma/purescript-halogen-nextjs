{
  "name": "purescript-halogen-mdl",
  "version": "1.0.0",
  "description": "My webpack project",
  "scripts": {
    "clean": "rimraf docs",
    "dev": "babel-node --presets '@babel/preset-env' --trace-deprecation ./webpack/dev.js",
    "dev:spago": "spago build --watch",
    "build": "babel-node --presets '@babel/preset-env' --trace-deprecation ./webpack/build.js",
    "serve": "node --trace-deprecation ./.dist/server/main.js --port 3000 --root-path /home/srghma/projects/purescript-halogen-nextjs/.dist/client",
    "cordova:clean": "rm -frdv www/css www/*.js",
    "cordova:build": "cordova build -- --webpack.config webpack/cordova-webpack.config.js --webpack.env.mode=production",
    "cordova:dev": "cordova run android -- --webpack.config webpack/cordova-webpack.config.js --webpack.env.mode=development",
    "cordova:dev:livereload": "cordova run android -- --webpack.config webpack/cordova-webpack.config.js --webpack.env.mode=development --livereload",
    "cordova:test-config": "node --trace-deprecation ./webpack/cordova-webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andywhite37/purescript-halogen-mdl.git"
  },
