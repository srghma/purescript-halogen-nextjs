    "cordova-plugin-webpack": "^1.0.5",
    "cordova-plugin-whitelist": "^1.3.4",
    "css-loader": "^3.6.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "firstline": "^2.0.2",
    "fs-extra": "^9.0.1",
